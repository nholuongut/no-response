    "scramjet": "^4.0.1"
  },
  "devDependencies": {
    "eslint-config-nholuongut": "^0.1.0",
    "expect": "^1.20.2",
    "localtunnel": "^1.8.3",
    "mocha": "^4.0.1",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^10.0.3"
  },
  "standard": {
    "env": [
      "mocha"
    ]
