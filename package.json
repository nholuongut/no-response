{
  "name": "nholuongut-no-response",
  "version": "1.0.0",
  "description": "a GitHub Integration that closes issues where the author hasn't responded to a request for more information",
  "author": "Lee Dohm",
  "license": "ISC",
  "repository": "github:nholuongut/no-response",
  "scripts": {
    "fix": "standard --fix",
    "dev": "nodemon",
    "start": "nholuongut run ./index.js",
    "test": "mocha && standard"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "dependencies": {
